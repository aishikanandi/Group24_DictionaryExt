{
    "manifest_version": 3,
    "name": "Dictionary",
    "version": "1.2.0",
    "action": {
        "default_icon": {
            "16": "assets/dictionary16.png",
            "64": "assets/dictionary64.png",
            "512": "assets/dictionary512.png"
        },
        "default_title": "Word Lookup",
        "default_popup": "popup/popup.html"
    },
    "description": "Select a word on the webpage and open popup to look up in the dictionary",
    "icons": {
      "16": "assets/dictionary16.png",
      "64": "assets/dictionary64.png",
      "512": "assets/dictionary512.png"
    },
    "author": "Group24",
    "content_scripts": [
        {
          "matches": [
            "<all_urls>"
          ],
          "js": ["content.js"]
        }
      ]
}
